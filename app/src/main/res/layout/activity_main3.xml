            android:contentDescription="@string/tod21"
            app:srcCompat="@drawable/image" />

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="85dp"
            android:layout_height="match_parent"
            android:contentDescription="@string/tod22"
            app:srcCompat="@drawable/image" />
    </TableRow>

</RelativeLayout>